/*orderall*/
.orderall{
	background: #FFFFFF;
	margin: 2.5% 0;
}
.orderall li{
	list-style: none;
}
.orderall li .item{
	line-height: 3.2rem;
	border: 1px solid #e1e1e1;
	border-width:1px 0;
	background: url(../images/jt.png) no-repeat 96.3% center /3.67%;
	padding: 0 3.125%;
}
.orderall li.on .item{
	background: url(../images/jt2.png) no-repeat 96.3% center /3.67%;
}
.orderall li .item a{
	display: block;
}
.orderall li .item em{
	display: inline-block;
	width: 4.67%;
	padding: 0 4.5% 0 0;
}
.orderall li .item em img{
	vertical-align: middle;
}
.orderall li .item a{
	color: #333333;
	font-size: 1.3rem;
}
.orderall li .txtCont{
	padding: 0 3.125%;
	display: none;
}
.orderall li .txtCont ul{
	overflow: hidden;
}
.orderall li .txtCont li{
	width: 33.3%;
	text-align: center;
	float: left;
	padding: 2.2% 0;
}
.orderall li .txtCont li em{
	display: inline-block;
	width: 16%;
	margin-bottom:3%;
}
.orderall li .txtCont li span{
	display: block;
	color: #333333;
	font-size: 1.1rem;
}


/*UserInfo*/
.UserInfo{
	padding: 2.4% 0;
}
.UserInfo li{
	list-style: none;
	padding: 2.4% 0;
	overflow: hidden;
}
.UserInfo li label{
	width: 21%;
	padding: 0 2.4% 0 0;
	vertical-align: middle;
	text-align: right;
	color: #333333;
	font-size: 1.4rem;
	display: inline-block;
	line-height: 3rem;
	float: left;
}
.UserInfo li.price{
	background: #faf9f9;
	padding: 5% 0 4.5%;
	margin: 2.4% 0;
}
.UserInfo li.price label{
	line-height: 1.7rem;
}
.UserInfo li.certificate label{
	line-height: 1.7rem;
	padding-top: 3.2rem;
}
.UserInfo li.certificate .img{
	float: right;
	width: 26.2%;
	border-left:1px solid #e1d2b8;
	height: 9.75rem;
	background: url(../images/photo.png) no-repeat center center /73.7% #caa777;
	position: relative;
}
.UserInfo li.certificate .img .t_file{
	width: 100%;
	height: 9.75rem;
	position: absolute;
	left: 0;
	top: 0;
	opacity: 0;
	border: 0 none;
}
.UserInfo li span{
	display: inline-block;
	width: 61.4%;
	float: left;
	border: 1px solid #caa777;
	border-radius: 9px;
	-moz-border-radius: 9px;
	-webkit-border-radius: 9px;
	-o-border-radius: 9px;
	background: #FFFFFF;
	overflow: hidden;
}
.UserInfo li span .text{
	border: 0 none;
	line-height: 3rem;
	background: #FFFFFF;
	width: 68%;
	float: left;
	color: #666666;
	font-size: 1.2rem;
	height: 2.8rem;
}
.UserInfo li span .text_nr{
	border: 0 none;
	line-height: 1.7rem;
	background: #FFFFFF;
	width: 86%;
	float: left;
	color: #666666;
	font-size: 1.2rem;
	resize: none;
	padding: 2% 0 0;
}
.UserInfo li span em{
	display: inline-block;
	float: right;
	width: 17.8%;
	line-height: 3rem;
	text-align: center;
	color: #ffffff;
	font-size: 1.2rem;
	background: #caa777;
	height: 100%;
}
.UserInfo li span a{
	display: inline-block;
	/*width: 14%;*/
	width: 5.4%;
	padding: 0 4.3%;
	text-align: center;
	line-height: 3rem;
	background: #ffffff;
	border-radius:9px 0 0 9px;
	-moz-border-radius:9px 0 0 9px;
	-webkit-border-radius:9px 0 0 9px;
	-o-border-radius:9px 0 0 9px;
	float: left;
	height: 100%;
}
.UserInfo li span a img{
	vertical-align: 0.1rem;
}
.UserInfo li .btn{
	width: 73%;
	padding: 3% 0;
	color: #FFFFFF;
	font-size: 1.4rem;
	background: #CAA777;
	border: 0 none;
	margin: 0 auto 3%;
	display: block;
	border-radius: 9px;
	-moz-border-radius: 9px;
	-webkit-border-radius: 9px;
	-o-border-radius: 9px;
	-webkit-appearance: none;
}



/*Headportrait*/
.Headportrait{
	width: 100%;
	height: 12rem;
	background: url(../images/Headportrait_bg2.png) no-repeat center top /100%;
	text-align: center;
}
.Headportrait em{
	display: inline-block;
	width: 7.2rem;
	height: 7.2rem;
	background: url(../images/Headportrait_bg.png) no-repeat center top /100%;
	text-align: center;
	line-height: 7.2rem;
	margin: 1.4rem 0 0.7rem;
}
.Headportrait em img{
	width: 6rem;
	height: 6rem;
	border-radius:50%;
	-moz-border-radius:50%;
	-webkit-border-radius:50%;
	-o-border-radius:50%;
	vertical-align: middle;
	margin-top: 9%; <!-- tc -->
}
.Headportrait span{
	display: block;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	color: #ffffff;
	font-size:1.2rem;
}



package com.chemguan.entity;

import java.util.List;


public class Columns {

	private Integer id;//主键
	private String columnName;//栏目名称
	private Integer parentColumnId;//父栏目ID
	private String imgURL;//栏目图片
	private String description;//栏目描述
	private String detail; //栏目详情
	
	
	
	private String parentcolumname;//父栏目名称
	
	//自定义添加子栏目列
	private List<Columns> list;
	
	
	
	public List<Columns> getList() {
		return list;
	}
	public void setList(List<Columns> list) {
		this.list = list;
	}
	public String getParentcolumname() {
		return parentcolumname;
	}
	public void setParentcolumname(String parentcolumname) {
		this.parentcolumname = parentcolumname;
	}
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getColumnName() {
		return columnName;
	}
	public void setColumnName(String columnName) {
		this.columnName = columnName;
	}
	public Integer getParentColumnId() {
		return parentColumnId;
	}
	public void setParentColumnId(Integer parentColumnId) {
		this.parentColumnId = parentColumnId;
	}
	public String getImgURL() {
		return imgURL;
	}
	public void setImgURL(String imgURL) {
		this.imgURL = imgURL;
	}
	public String getDescription() {
		return description;
	}
	public void setDescription(String description) {
		this.description = description;
	}
	public String getDetail() {
		return detail;
	}
	public void setDetail(String detail) {
		this.detail = detail;
	}
}

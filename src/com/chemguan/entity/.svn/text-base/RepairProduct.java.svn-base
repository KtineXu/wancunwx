package com.chemguan.entity;

/**
 * 这是我加的一个包装类，在产品维修进度查询的时候，既需要产品信息，又需要产品维修记录信息，所以，我加了这么一个包装类
 * @author 郭胜
 *
 */
public class RepairProduct {

	//产品的维修记录
	private RepairRecords repairRecords;
	//产品信息
	private Products products;
	public RepairRecords getRepairRecords() {
		return repairRecords;
	}
	public void setRepairRecords(RepairRecords repairRecords) {
		this.repairRecords = repairRecords;
	}
	public Products getProducts() {
		return products;
	}
	public void setProducts(Products products) {
		this.products = products;
	}
	
}

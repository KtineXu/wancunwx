package com.chemguan.entity;

import java.util.List;

/**
 * 这是作品和评论的包装类
 * @author Administrator
 *
 */
public class WorksAndCommon {

	/**
	 * 作品的属性
	 */
	private Works works;
	
	/**
	 * 评论的属性的集合
	 */
	private List<WorksComment> commentList;
	
	
	
	/**
	 * 评论总数
	 */
	private int comTotal;

	public Works getWorks() {
		return works;
	}

	public void setWorks(Works works) {
		this.works = works;
	}


	public List<WorksComment> getCommentList() {
		return commentList;
	}

	public void setCommentList(List<WorksComment> commentList) {
		this.commentList = commentList;
	}

	public int getComTotal() {
		return comTotal;
	}

	public void setComTotal(int comTotal) {
		this.comTotal = comTotal;
	}
}

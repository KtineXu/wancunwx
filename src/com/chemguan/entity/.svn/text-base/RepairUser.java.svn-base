package com.chemguan.entity;

/**
 * 我加的一个维修记录和用户的包装类，方便后台页面的管理
 * @author 郭胜
 *
 */
public class RepairUser {
	//这是维修记录
	private RepairRecords repairRecords;
	//这是用户
	private Users users;
	
	public RepairRecords getRepairRecords() {
		return repairRecords;
	}
	public void setRepairRecords(RepairRecords repairRecords) {
		this.repairRecords = repairRecords;
	}
	public Users getUsers() {
		return users;
	}
	public void setUsers(Users users) {
		this.users = users;
	}
	
}
